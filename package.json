{
  "name": "web3-toolkit",
  "version": "0.11.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:ts && npm run build:abi",
    "build:ts": "tsup",
    "build:abi": "mkdir -p dist/abi && cp -R src/abi/* dist/abi/"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./abi/*": "./dist/abi/*"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox-viem": "^5.0.1",
    "@nomicfoundation/hardhat-viem": "^3.0.1",
    "hardhat": "^3.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "viem": "^2.43.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "ethers": "^5.7.2"
  }
}
