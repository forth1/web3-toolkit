{
  "name": "web3-toolkit",
  "version": "0.11.0",
  "scripts": {
    "build": "npm run build:ts && npm run build:abi",
    "build:ts": "tsup",
    "build:abi": "mkdir -p dist/abi && cp -R src/abi/* dist/abi/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./abi/*": "./dist/abi/*"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
